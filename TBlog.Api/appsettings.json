{
  "BaseSetting": {
    "ApiName": "TBlog.Api",
    "Host": "http://tblog.falseendless.cn",
    "EnabledTest": true,
    "MiniProfiler": true
  },
  "DBSetting": {
    "MongoConnection": "",
    "MongoDbName": "tblog",
    "CQRSEnabled": false,
    "SeedDBEnabled": false,
    "SeedDBDataEnabled": false,
    "SeedDataFolder": "SeedData/{0}.tsv",
    "DBS": [
      {
        "Enabled": false,
        "DBType": "SqlServer",
        "Connection": ""
      }
    ]
  },
  "TencentCloud": {
    "Appid": 0,
    "Region": "",
    "SecretId": "",
    "SecretKey": "",
    "DurationSecond": 600,
    "Bucket": "",
    "DoMain": ""
  },
  "Elasticsearch": {
    "Url": "",
    "UserName": "",
    "Password": "",
    "DefaultIndex": "acticle"
  },
  "JwtBearer": {
    "Enabled": true,
    "Secret": "This is your Secret",
    "Audience": "TBlog",
    "Issuer": "TBlog.Api",
    "ExpressTime": 604800
  },
  "IdentityServer4": {
    "Enabled": false,
    "ApiName": "tblog.api",
    "AuthorizationUrl": "http://localhost:5004"
  },
  "AOPSetting": {
    "RedisCacheAOP": false,
    "MemoryCachingAOP": true,
    "ServerLogAOP": false,
    "TransactionProcessAOP": true,
    "SqlLogAOP": false
  },
  "Cors": {
    "Enable": true,
    "PolicyName": "TBlogCors",
    "FilterIPs": [ "http://127.0.0.1:3257", "http://localhost:3257" ]
  },
  "Middleware": {
    "HTTPLogMatchPath": [ "api" ],
    "SignalR": false,
    "IpRateLimit": true
  },
  "Consul": {
    "Enabled": false,
    "ServiceName": "TBlogCoreService",
    "ServiceIP": "localhost",
    "ServicePort": "8081",
    "ServiceHealthCheck": "/healthcheck",
    "ConsulAddress": "http://localhost:8500",
    "RegisterDealyTime": 5,
    "CheckHelpInterval": 10,
    "Timeout": 5
  },
  "RabbitMQ": {
    "Enabled": true,
    "Connection": "",
    "UserName": "root",
    "Password": "",
    "RetryCount": 3
  },
  "Redis": {
    "RedisMq": false,
    "Connection": "",
    "PassWord": ""
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "IpWhitelist": [],
    "EndpointWhitelist": [ "get:/api/xxx", "*:/api/yyy" ],
    "ClientWhitelist": [ "dev-client-1", "dev-client-2" ],
    "QuotaExceededResponse": {
      "Content": "{{\"status\":429,\"msg\":\"请求过于频繁，请稍后再试\",\"success\":false}}",
      "ContentType": "application/json",
      "StatusCode": 429
    },
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*/api/*",
        "Period": "1s",
        "Limit": 3
      },
      {
        "Endpoint": "*/api/*",
        "Period": "1m",
        "Limit": 30
      },
      {
        "Endpoint": "*/api/*",
        "Period": "12h",
        "Limit": 500
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}